{{#each this}}
<div class="photo-card">
    <a href= {{largeImageURL}}>
        <img src= {{webformatURL}} alt={{tags}}  loading="lazy" width="250" height="180" class="card-image" />
    </a>
    <div class="info">
        <p class="info-item">
            <b class="info-number">Likes: {{likes}}</b>
        </p>
        <p class="info-item">
            <b class="info-number">Views: {{views}}</b>
        </p>
        <p class="info-item">
            <b class="info-number">Comments: {{comments}}</b>
        </p>
        <p class="info-item">
            <b class="info-number">Downloads: {{downloads}}</b>
        </p>
    </div>
</div>
{{/each }}